{
  "name": "mammooc-rating-widget",
  "version": "2.0.1",
  "description": "A Polymer element providing MOOC evaluation support with mammooc",
  "contributors": [
    {
      "name": "Paul Arndt"
    },
    {
      "name": "Rosa Braatz"
    },
    {
      "name": "Philipp Fischbeck"
    },
    {
      "name": "Dominic Sauer"
    },
    {
      "name": "Sebastian Serth"
    }
  ],
  "main": "mammooc-rating-widget.html",
  "homepage": "https://github.com/mammooc/mammooc-rating-widget#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mammooc/mammooc-rating-widget.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mammooc/mammooc-rating-widget/issues"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^4.7.2",
    "eslint-plugin-html": "^3.1.0",
    "htmlhint": "^0.9.13",
    "polymer-bundler": "^3.0.0",
    "polymer-cli": "^1.5.5",
    "web-component-tester": "^6.2.0"
  },
  "scripts": {
    "test": "node_modules/web-component-tester/bin/wct test/index.html",
    "test-firefox": "node_modules/web-component-tester/bin/wct  test/index.html -l firefox",
    "test-safari": "node_modules/web-component-tester/bin/wct  test/index.html -l safari",
    "test-chrome": "node_modules/web-component-tester/bin/wct  test/index.html -l chrome",
    "testdebugging": "node_modules/web-component-tester/bin/wct -p test/index.html",
    "lint": "eslint src --ext .html --fix && htmlhint --config .htmlhintrc src/**/*.html index.html && polymer lint",
    "postinstall": "bower install",
    "serve": "polymer serve",
    "bundle": "polymer build",
    "analyze": "polymer analyze > analysis.json",
    "deploy": "./utils/deploy.sh"
  }
}
