  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@polymer/app-localize-behavior": "^3.0.1",
    "@polymer/iron-ajax": "^3.0.1",
    "@polymer/iron-demo-helpers": "^3.0.2",
    "@polymer/iron-form": "^3.0.1",
    "@polymer/iron-form-element-behavior": "^3.0.1",
    "@polymer/iron-jsonp-library": "^3.0.1",
    "@polymer/iron-list": "^3.0.1",
    "@polymer/iron-scroll-threshold": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-card": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-input": "^3.0.1",
    "@polymer/paper-radio-button": "^3.0.1",
    "@polymer/paper-radio-group": "^3.0.1",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/paper-toast": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "flexible-rating": "^3.0.3",
    "intl-messageformat": "^2.2.0"
  },
  "devDependencies": {
    "@polymer/iron-component-page": "^3.0.0-pre.21",
    "@webcomponents/webcomponentsjs": "^2.2.6",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.3.0",
    "eslint-plugin-html": "^4.0.5",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-insert": "^0.5.0",
    "gulp-rollup": "^2.16.2",
    "gulp-uglify-es": "^1.0.4",
    "htmlhint": "^0.9.13",
    "polymer-bundler": "^4.0.2",
    "polymer-cli": "^1.7.7",
    "rollup-plugin-node-resolve": "^4.0.0",
    "url-polyfill": "^1.1.3",
    "wct-browser-legacy": "^1.0.2",
    "web-component-tester": "^6.9.2"
  },
  "scripts": {
    "test": "node_modules/web-component-tester/bin/wct --module-resolution=node --npm test/index.html",
    "test-firefox": "node_modules/web-component-tester/bin/wct --module-resolution=node --npm test/index.html -l firefox",
    "test-safari": "node_modules/web-component-tester/bin/wct --module-resolution=node --npm test/index.html -l safari",
    "test-chrome": "node_modules/web-component-tester/bin/wct --module-resolution=node --npm test/index.html -l chrome",
    "testdebugging": "node_modules/web-component-tester/bin/wct --module-resolution=node --npm -p test/index.html",
    "lint": "eslint src --fix && htmlhint --config .htmlhintrc src/**/*.html index.html && polymer lint",
    "serve": "polymer serve --module-resolution=node --npm",
    "bundle": "gulp bundle",
    "analyze": "polymer analyze > analysis.json",
    "deploy": "./utils/deploy.sh"
  },
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  }
}
